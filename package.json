{
  "name": "mv-asset-api",
  "version": "1.0.0",
  "description": "Motorverse Asset API Server",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=local nodemon src/index.js",
    "dev2": "cross-env nodemon src/index.ts",
    "dev3": "npm run build && npm run serve",
    "serve": "node -r dotenv/config build/src/index.js",
    "build": "npm run clean && npm run build-ts",
    "migrator": "node src/database/migrator.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "find-circular-deps": "madge . -c"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.20.0",
    "@joi/date": "^2.1.0",
    "alchemy-sdk": "^3.2.0",
    "bignumber.js": "^9.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ethers": "^5.7.2",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "pg": "^8.9.0",
    "pm2": "^5.1.0",
    "sequelize": "^6.37.1",
    "umzug": "^3.2.1",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@types/passport": "^1.0.16",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "madge": "^6.1.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.0.5",
    "typescript": "^5.4.2"
  }
}
