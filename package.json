{
  "name": "mv-asset-api",
  "version": "1.0.0",
  "description": "Motorverse Asset API Server",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=local nodemon src/index.js",
    "dev2": "cross-env nodemon src/index.ts",
    "dev3": "npm run build && npm run serve",
    "dev4": "concurrently \"npm:serve-ts\" \"npm:watch-ts\"",
    "serve-ts": "nodemon -r dotenv/config -r ts-node/register src/index.ts",
    "serve": "node -r dotenv/config build/src/index.js",
    "build": "npm run clean && npm run build-ts",
    "migrator": "node src/database/migrator.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "find-circular-deps": "madge . -c",
    "schemas:openapi": "swagger-jsdoc -d src/route/definition.yaml src/route/**/*.ts src/route/**/*.yaml \"src/route/v1/!(definition).yaml\" -o src/schema/openapi.json",
    "schemas:types": "openapi-typescript src/schema/openapi.json -o src/schema/openapi.d.ts",
    "copyfiles": "npx copyfiles -u 1 src/schema/*.json build",
    "deployDev": "npx hardhat run ./scripts/deployToLocalhost.ts --network localhost"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.20.0",
    "@joi/date": "^2.1.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.2",
    "@openzeppelin/contracts": "^5.0.2",
    "@types/sequelize": "^4.28.20",
    "alchemy-sdk": "^3.2.0",
    "bignumber.js": "^9.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ethers": "^6.12.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^5.2.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "pg": "^8.9.0",
    "pm2": "^5.1.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "solc": "^0.8.24",
    "swagger-ui-express": "^5.0.1",
    "umzug": "^3.2.1",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.6",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.16",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.28",
    "@types/passport": "^1.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "chai": "^4.4.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.0.0",
    "hardhat": "^2.22.3",
    "hardhat-gas-reporter": "^1.0.10",
    "jest": "^26.0.1",
    "madge": "^6.1.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.22",
    "openapi-typescript": "^7.0.0",
    "prettier": "^2.0.5",
    "solidity-coverage": "^0.8.12",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.4.2"
  }
}
